<Window x:Class="Project_X.Sensor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sensor" Height="300" Width="300">
    <Grid>
        <TextBox Name="Model" HorizontalAlignment="Left" Height="23" Margin="142,49,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="Model" TextChanged="Model_TextChanged"/>
        <TextBox Name="SensorID" HorizontalAlignment="Left" Height="23" Margin="142,89,0,0" TextWrapping="Wrap" Text="Sensor ID" VerticalAlignment="Top" Width="120" TextChanged="SensorID_TextChanged"/>
        <TextBox Name="CprNr"  HorizontalAlignment="Left" Height="23" Margin="142,136,0,0" TextWrapping="Wrap" Text="Cpr Nr" VerticalAlignment="Top" Width="120" TextChanged="CprNr_TextChanged"/>
        <Button Name="SaveSensor" Content="Save" HorizontalAlignment="Left" Margin="121,231,0,0" VerticalAlignment="Top" Width="75" Click="SaveSensor_Click"/>
        <TextBox Name="BatteryLastChanged" HorizontalAlignment="Left" Height="23" Margin="142,179,0,0" TextWrapping="Wrap" Text="Batterylastchanged" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
